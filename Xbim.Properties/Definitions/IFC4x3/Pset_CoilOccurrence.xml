<?xml version="1.0" ?>
<PropertySetDef templatetype="PSET_OCCURRENCEDRIVEN" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4X3"/>
  <Name>Pset_CoilOccurrence</Name>
  <Definition>Coil occurrence attributes attached to an instance of IfcCoil.</Definition>
  <Applicability/>
  <ApplicableClasses>
    <ClassName>IfcCoil</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcCoil</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef>
      <Name>HasSoundAttenuation</Name>
      <Definition>TRUE if the coil has sound attenuation, FALSE if it does not.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcBoolean"/>
        </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
